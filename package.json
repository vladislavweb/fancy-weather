{
  "name": "fancy-weather",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@formatjs/cli": "^5.1.3",
    "@tanstack/react-query": "^4.15.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.1",
    "react-map-gl": "^7.0.19",
    "react-scripts": "5.0.1",
    "sass": "^1.56.1",
    "typescript": "^4.8.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write ./src",
    "extract": "formatjs extract 'src/**/*.tsx' --ignore='**/*.d.ts' --out-file 'src/lang/en.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile:en": "formatjs compile 'src/lang/en.json' --ast --out-file 'src/compiled-lang/en.json'",
    "compile:ru": "formatjs compile 'src/lang/ru.json' --ast --out-file 'src/compiled-lang/ru.json'",
    "compile:uk": "formatjs compile 'src/lang/uk.json' --ast --out-file 'src/compiled-lang/uk.json'",
    "compile": "npm run compile:en && npm run compile:ru && npm run compile:uk"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/dom-speech-recognition": "^0.0.1",
    "prettier": "^2.7.1"
  }
}
